<?xml version="1.0" encoding="utf-8"?>
<odoo>
	
	
	<template id="lerm_microsilica_report">
		<t t-call="web.html_container">
			<!-- <t t-call="web.basic_layout"> -->
			
			<t t-call="lerm_civil.custom_layout_header_footer_lerm_eln_general">
				<div style="text-align:center;margin-top:10px">
					<p style="font-size:15px"><b>Test Report On Microsilica Mechanical</b></p>
				</div>
				<div class="container_eln">
					<div t-foreach="eln" t-as="o">
						<table>
							<tr>
								<td colspan="3">Test Report No :
								</td>
								<td colspan="3">
									<span t-field="o.eln_id"/>
								</td>
								<td colspan="3">Report Issue Date :
								</td>
								<td colspan="3">
									<span t-field="o.srf_date"/>
								</td>
								<td colspan="3">ULR NO :
								</td>
								<td colspan="3">
									<span t-field="o.sample_id.ulr_no"/>
								</td>
							</tr>
						</table>
							<table>
								<tr>
									<!-- Left Side -->
									<td colspan="12" style="width: 50%; vertical-align: top;">
										<table>
											<tr>
												<td colspan="3" style=" width: 30%; height: 100px; font-weight: bold;">
													Customer/Party Name :
												</td>
												<td colspan="3">
												<span t-field="o.srf_id.customer.name"/>
												<span t-field="o.srf_id.customer.street"/>
												<span t-field="o.srf_id.customer.street2"/>
												<span t-field="o.srf_id.customer.city"/>
												<span t-if="o.srf_id.customer.zip" t-field="o.srf_id.customer.zip"/>
												<span t-field="o.srf_id.customer.state_id"/>
												,
												<span t-field="o.srf_id.customer.country_id"/>
									           </td>
											</tr>
											
										</table>
									</td>
									<!-- Right Side -->
									<td colspan="12" style="width: 30%; vertical-align: top;">
										<table>
											<tr>
											    <td colspan="3" style="font-weight: bold;" >
													Sample Receipt Date :
												</td>
												<td colspan="3">
													<span t-field="o.sample_id.sample_received_date"/>
												</td>
											</tr>
											<tr>
											    <td colspan="3" style="font-weight: bold;" >
													Test Start Date :
												</td>
												<td colspan="3">
													<span t-field="o.start_date"/>
												</td>
											</tr>
											<tr>
											    <td colspan="3" style="font-weight: bold;" >
													Test End Date :
												</td>
												<td colspan="3">
													<span t-field="o.end_date"/>
												</td>
											</tr>
										</table>
									</td>
								</tr>
							

						</table>
						<table>
						   <tr>
								<td colspan="12" style="font-weight: bold;">Project / Site Address :
									<![CDATA[&nbsp;]]>
									<span t-field="o.srf_id.site_address.name"/>
									<span t-field="o.srf_id.site_address.street"/>
									<span t-field="o.srf_id.site_address.street2"/>
									<span t-field="o.srf_id.site_address.city"/>
									<span t-if="o.srf_id.site_address.zip" t-field="o.srf_id.site_address.zip"/>
									<span t-field="o.srf_id.site_address.state_id"/>
									,
									<span t-field="o.srf_id.site_address.country_id"/>
								</td>
							</tr>
							<tr>
								<td colspan="12" style="font-weight: bold;">Sample Description : <![CDATA[&nbsp;]]>
									<span t-field="o.sample_id.sample_description"/>
								</td>
							</tr>
							<tr>
								<td colspan="12" style="font-weight: bold;">Sample Id Mark: <![CDATA[&nbsp;]]>
										<span t-field="o.sample_id.client_sample_id"/>
								</td>
							</tr>
							<tr>
							   <td colspan="3" style="font-weight: bold;">Quantity :
									<![CDATA[&nbsp;]]>
								</td>
								<td colspan="3">
									<span t-field="o.sample_id.size_id.size"/>
								</td>
								<td colspan="3" style="font-weight: bold;">Grade :
									<![CDATA[&nbsp;]]>
								</td>
								<td>
									<span t-field="o.grade_id.grade"/>
								</td>
							</tr>
							<tr>
							    <td colspan="3" style="font-weight: bold;">Brand :
									<![CDATA[&nbsp;]]>
								</td>
								<td colspan="3">
									<span t-field="o.sample_id.brand"/>
								</td>
								<td colspan="3" style="font-weight: bold;">Consultant Name :
									<![CDATA[&nbsp;]]>
								</td>
								<td>
								</td>
							</tr>
							<tr>
							    <td colspan="3" style="font-weight: bold;">Client Name :
									<![CDATA[&nbsp;]]>
								</td>
								<td colspan="3"></td>
								<td colspan="3" style="font-weight: bold;">Client Reference :
									<![CDATA[&nbsp;]]>
								</td>
								<td>
									<span t-field="o.srf_id.client_refrence"/>
								</td>
							</tr>
							<tr>
							    <td colspan="3" style="font-weight: bold;">Discipline :
									<![CDATA[&nbsp;]]>
								</td>
								<td colspan="3">
									<span t-field="o.discipline.discipline"/>
								</td>
								<td colspan="3" style="font-weight: bold;">Group :
									<![CDATA[&nbsp;]]>
								</td>
								<td colspan="3">
									<span t-field="o.group.group"/>
								</td>
							</tr>
							<tr>
							    <td colspan="3" style="font-weight: bold;">Material/Test Name :
									<![CDATA[&nbsp;]]>
								</td>
								<td colspan="12">
									<span t-field="o.material.name"/>
								</td>
							</tr>
							<tr>
							    <td colspan="3" style="font-weight: bold;">Room Temp :
									<![CDATA[&nbsp;]]>
								</td>
								<td colspan="3">
									
								</td>
								<td colspan="3" style="font-weight: bold;">Room RH :
									<![CDATA[&nbsp;]]>
								</td>
								<td colspan="3">
									
								</td>
							</tr>

						</table>
					</div>
					<div style="margin-top:20px;">
						<p><strong>Results</strong>:<![CDATA[&nbsp;]]></p>
						<!-- <span t-field="eln.parameters_result.parameter[0].test_method.test_method"/> -->
					</div>
					<table style="margin-top:30px;">
						<thead>
							<tr>
								<th style="text-align: center;">
									<span style="font-weight: bold;">
										Sr No
									</span>
								</th>
								<th style="text-align: center;max-width:200px">
									<span style="font-weight: bold;">
										Test Parameters
									</span>
								</th>
								<th style="text-align: center;max-width:100px">
									<span style="font-weight: bold;">
										Unit
									</span>
								</th>
								<th style="text-align: center;">
									<span style="font-weight: bold;">
										Result
									</span>
								</th>
								<th style="text-align: center;">
									<span style="font-weight: bold;">
										Test Method
									</span>
								</th>
								<th style="text-align: center;max-width:150px">
									<span colspan="3" style="font-weight: bold;">
										Limits As per IS
									</span>
								</th>
							</tr>
						</thead>
					<tbody >

							<!--1 Accelerated pozzolanic activity index with portland cement , 7 Days in % -->

						<tr t-if="microsilica.compressive_visible">
							

							<td colspan="1" style="text-align: center;">
								<div>
									<span>1</span>
								</div>
							</td>

							<td style="text-align: center;max-width:200px">
								<span style="text-align: center;">
									Accelerated pozzolanic activity index with portland cement , 7 Days in %
								</span>
								
							</td>
							
							<td style="text-align: center;max-width:100px">
								<span style="text-align: center;" >
									<span t-field="env['lerm.parameter.master'].search([('parameter_name','=','Accelerated pozzolanic activity index with portland cement')]).unit.name"/>
								</span>
							</td>
							<td style="text-align: center;">
								<span style="text-align: center;" >
									<span t-field="microsilica.control_compressive_strength_7_days" t-options='{"widget": "float", "precision": 2}'/>
								</span>									
							</td>
							<td style="text-align: center;">
								<span style="text-align: center;">
									<span t-field="env['lerm.parameter.master'].search([('parameter_name','=','Accelerated pozzolanic activity index with portland cement')]).test_method.test_method"/>
								</span>
							</td>
							<td style="text-align: center;max-width:150px">
								<span style="text-align: center;" >
									Min 105 ,As per ASTM C1240 : 2020
								</span>
							</td>
						</tr>
							<!--2 Oversize % Retained on 45 Micron wet sieve -->
						<tr t-if="microsilica.oversize_retain_visible">
							<td colspan="1" style="text-align: center;">
								<div>
									<span>2</span>
								</div>
							</td>
							<td style="text-align: center;max-width:200px">
								<span style="text-align: center;">
									Oversize % Retained on 45 Micron wet sieve
								</span>
								
							</td>
							
							<td style="text-align: center;max-width:100px">
								<span style="text-align: center;" >
									<span t-field="env['lerm.parameter.master'].search([('parameter_name','=','Oversize % Retained on 45 Micron IS sieve')]).unit.name"/>
								</span>
							</td>
							<td style="text-align: center;">
								<span style="text-align: center;" >
									<span t-field="microsilica.retain_wt_rounded" t-options='{"widget": "float", "precision": 2}'/>
								</span>									
							</td>
							<td style="text-align: center;">
								<span style="text-align: center;">
									<span t-field="env['lerm.parameter.master'].search([('parameter_name','=','Oversize % Retained on 45 Micron IS sieve')]).test_method.test_method"/>
								</span>
							</td>
							<td style="text-align: center;max-width:150px">
								<span style="text-align: center;" >
									(10 Max) As Per IS 15388 : 2003
								</span>
							</td>
						</tr>
							<!--3 Specific Gravity -->
						<tr t-if="microsilica.specific_gravity_visible">
							<td colspan="1" style="text-align: center;">
								<div>
									<span>3</span>
								</div>
							</td>
							<td style="text-align: center;max-width:200px">
								<span style="text-align: center;">
									Specific Gravity
								</span>
								
							</td>
							
							<td style="text-align: center;max-width:100px">
								<span style="text-align: center;" >
									-
									<!-- <span t-field="env['lerm.parameter.master'].search([('parameter_name','=','Specific Gravity')]).unit.name"/> -->
								</span>
							</td>
							<td style="text-align: center;">
								<span style="text-align: center;" >
									<span t-field="microsilica.specific_gravity_avrg" t-options='{"widget": "float", "precision": 2}'/>
								</span>									
							</td>
							<td style="text-align: center;">
								<span style="text-align: center;">
									<span t-field="env['lerm.parameter.master'].search([('parameter_name','=','Specific Gravity')]).test_method.test_method"/>
								</span>
							</td>
							<td style="text-align: center;max-width:150px">
								<span style="text-align: center;" >
									-
								</span>
							</td>
						</tr>
							<!--4 Compressive Strength -->
						<tr t-if="microsilica.compressive_strength_visible">	
							<td colspan="1" style="text-align: center;">
								<div>
									<span>4</span>
								</div>
							</td>
							<td style="text-align: center;padding:0px;max-width:200px">
								<div style="text-align: center; border-bottom:1px solid black;min-height:40px">
									Compressive Strength of Microsilica Sample 7 days
								</div>
								<div style="text-align: center; border-bottom:1px solid black;min-height:30px">
									Compressive Strength of control Sample 7 days
								</div>
								<div style="text-align: center;min-height:30px">
									In Percent
								</div>	
							</td>

							<td style="text-align: center;padding:0px;max-width:100px">
								<div style="text-align: center; border-bottom:1px solid black;min-height:40px">
									<!-- MPA -->
									<span t-field="env['lerm.parameter.master'].search([('parameter_name','=','Compressive Strength')]).unit.name"/>

								</div>
								<div style="text-align: center; border-bottom:1px solid black;min-height:30px">
									<span t-field="env['lerm.parameter.master'].search([('parameter_name','=','Compressive Strength')]).unit.name"/>

									<!-- MPA -->
								</div>
								<div style="text-align: center;min-height:30px">
									%
								</div>	
							</td>
							
							<!-- <td style="text-align: center;">
								<span style="text-align: center;" >
									<span t-field="env['lerm.parameter.master'].search([('parameter_name','=','Compressive Strength')]).unit.name"/>
								</span>
							</td> -->

							
							<td style="text-align: center; padding: 0px">
								<div style="text-align: center; border-bottom: 1px solid black;min-height:40px">
									<span t-field="microsilica.comp_average_casting_7days" t-options='{"widget": "float", "precision": 2}'/>
								</div>
								<div style="text-align: center; border-bottom: 1px solid black;min-height:30px">
									<span t-field="microsilica.comp_control_average_casting_7days" t-options='{"widget": "float", "precision": 2}'/>
								</div>
								<div style="text-align: center;min-height:30px">
									<span t-field="microsilica.comp_control_strngth_7_days" t-options='{"widget": "float", "precision": 2}'/>
								</div>
							</td>
							<td style="text-align: center;min-height:30px">
								<span style="text-align: center; display: block;">
									<span t-field="env['lerm.parameter.master'].search([('parameter_name','=','Compressive Strength')]).test_method.test_method"/>
								</span>
							</td>

							<td style="text-align: center;max-width:150px">
								<span style="text-align: center;" >
									As Per IS 15388 : 2003 Compressive Strength at 7 days as Percent of Control Sample Min.85 %
								</span>
							</td>
						</tr>

							<!--5 Oversize Percent Retained on 45 Micron IS sieve variation from Avg. % -->
						<tr t-if="microsilica.oversize_percent_retain_visible">
							<td colspan="1" style="text-align: center;">
								<div>
									<span>5</span>
								</div>
							</td>
							<td style="text-align: center;max-width:200px">
								<span style="text-align: center;">
									Oversize Percent Retained on 45 Micron IS sieve variation from Avg. %
								</span>
								
							</td>
							
							<td style="text-align: center;max-width:100px">
								<span style="text-align: center;" >
									<span t-field="env['lerm.parameter.master'].search([('parameter_name','=','Oversize Percent Retained on 45 Micron IS sieve variation from Avg. %')]).unit.name"/>
								</span>
							</td>
							<td style="text-align: center;">
								<span style="text-align: center;" >
									<span t-field="microsilica.avrg_oversize_percent" t-options='{"widget": "float", "precision": 2}'/>
								</span>									
							</td>
							<td style="text-align: center;">
								<span style="text-align: center;">
									<span t-field="env['lerm.parameter.master'].search([('parameter_name','=','Oversize Percent Retained on 45 Micron IS sieve variation from Avg. %')]).test_method.test_method"/>
								</span>
							</td>
							<td style="text-align: center;max-width:150px">
								<span style="text-align: center;" >
									-
								</span>
							</td>
						</tr>

							<!--6 Dry Loose Bulk Density -->
						<tr t-if="microsilica.bulk_density_visible">
							<td colspan="1" style="text-align: center;">
								<div>
									<span>6</span>
								</div>
							</td>
							<td style="text-align: center;">
								<span style="text-align: center;max-width:200px">
									Dry Loose Bulk Density
								</span>
								
							</td>
							
							<td style="text-align: center;max-width:100px">
								<span style="text-align: center;" >
									<span t-field="env['lerm.parameter.master'].search([('parameter_name','=','Dry Loose Bulk Density')]).unit.name"/>
								</span>
							</td>
							<td style="text-align: center;">
								<span style="text-align: center;" >
									<span t-field="microsilica.avrg_bulk_density" t-options='{"widget": "float", "precision": 2}'/>
								</span>									
							</td>
							<td style="text-align: center;">
								<span style="text-align: center;">
									<span t-field="env['lerm.parameter.master'].search([('parameter_name','=','Dry Loose Bulk Density')]).test_method.test_method"/>
								</span>
							</td>
							<td style="text-align: center;max-width:150px">
								<span style="text-align: center;" >
									-
								</span>
							</td>
						</tr>
					</tbody >
				</table>
				<div>

				</div>
				<div style="margin-top:20px; page-break-inside: avoid;" class="noteContainer">
					<div>
						<b>Remarks: -</b>
					</div>
					<div>
						<ul>
							<li>
								The Test Report(s) is/are valid only to the sample submitted to the laboratory.
							</li>
							<li>
								Sample(s) was/were not drawn by laboratory.
							</li>
							<li>
								This Report may not be reproduced in except full/ part without the permission of the Lab Head of the Laboratory.
							</li>
						</ul>
					</div>
				</div>


				<div class="signatureContainer girdgird" style="margin-top:50px;page-break-inside: avoid;">
					<div>
						<img t-if="qrcode" t-att-src="'data:image/png;base64,'+str(qrcode)" style="width: 100px; height: 100px;"/>
					</div>
					<div >
						Format No. <span t-esc="eln.material.data_sheet_format_no.name"/>
					</div>
					<div>
						<div>
							<b>
								Approved By
							</b>
						</div>
						<div style="height:20px">
							
						</div>
						<div>
							<b>
								Technical Manager
							</b>
						</div>
						<div>
							<b>
								(Authorized Signatory)
							</b>
						</div>
					</div>
				</div>
				
														
				</div>
				
			</t>
		</t>
	</template>
</odoo>