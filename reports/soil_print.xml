								<!-- <th>
									<span>
										NABL Status
									</span>
								</th> -->
								<!-- <th t-if="eln.conformity">
									<span>
										Confirmity Status
									</span>
								</th> -->
							</tr>
						</thead>
						<tbody>
							<!-- <t t-set="i" t-value="0"/> -->
							<!-- non nabl -->
							<tr t-foreach="data.child_lines" t-as="r" t-if="data.confirmity == 'fail' and not nabl">
								<!-- <td>
									<div>
