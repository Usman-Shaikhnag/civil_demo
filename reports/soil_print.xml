					<table>
						<thead>
							<tr>
								<th>
									<span>
										Sr. No
									</span>
								</th>
								<th>
									<span>
										ID MARK/Location
									</span>
								</th>
								<th>
									<span>
										Length in mm
									</span>
								</th>
								<th>
									<span>
										Width in mm
									</span>
								</th>
								<th>
									<span>
										Area in mm
									</span>
								</th>
								
								<th>
									<span>
										Weight of sample in kgs
									</span>
								</th>

								<th>
									<span>
										Crushing load in KN
									</span>
								</th>
								<th>
									<span>
										Compresive stringth in n/mm2
									</span>
