           <table style="margin-top:20px">
				<tr cols="12">
					
					<td colspan="6">Job No :
                        <![CDATA[&nbsp;]]>
                         <span t-field="eln.kes_no"></span>
                    </td>
					<td colspan="6">Test Start Date: 
                        <![CDATA[&nbsp;]]>
                        <span t-field="eln.start_date"></span>
                    </td>
				</tr>
                <tr>
                  <td colspan="6">Material: <span t-field="eln.material"></span>
                </td>
                 <td colspan="6">Test End Date: <span t-field="eln.end_date"></span>
                </td>
                </tr>
			</table>
              <!-- Dimension  -->

                   <div t-if="data.dimension_visible" style="margin-top:20px">
                        <div style="text-align:center;font-size:18px">
                            <b>Dimension</b>
                        </div>
                        <table>
                            <tr cols="12">
                                <td colspan="1" style="text-align: center;"><b>Sr No.</b></td>
                                <td colspan="2" style="text-align: center;"><b>Length in mm</b></td>
                                <td colspan="1" style="text-align: center;"><b>Width in mm</b></td>
                                <td colspan="2" style="text-align: center;"><b>Thickness in mm</b></td>
                            </tr>
                            <t t-set="f" t-value="0"/>
                                <tr t-foreach="data.child_lines" t-as="r">
                                    <td colspan="1" style="text-align: center;">
                                        <div>
                                            <span t-esc="f + 1" />
                                            <t t-set="f" t-value="f + 1"/>
                                        </div>
                                    </td>
                                    <td colspan="2" style="text-align: center;">
                                        <span t-esc="r['length']"/>
                                    </td>
                                    <td colspan="1" style="text-align: center;">
                                        <span t-esc="r['width']"/>
                                    </td>
                                    <td colspan="2" style="text-align: center;">
                                        <span t-esc="r['hight']"/>
                                    </td>
                            </tr>
                        </table>
                        <table style="margin-top:5px">
