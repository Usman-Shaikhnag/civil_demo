<?xml version="1.0" encoding="utf-8"?>
<odoo>
	
	
	<template id="lerm_flyash_report">
		<t t-call="web.html_container">
			<!-- <t t-call="web.basic_layout"> -->
			
			<t t-call="lerm_civil.custom_layout_header_footer_lerm_eln_general">
				<div style="text-align:center;margin-top:10px">
					<p style="font-size:15px"><b>Test Report On Fly Ash Mechanical</b></p>
				</div>
				<div class="container_eln">
					<div t-foreach="eln" t-as="o">
						<table>
							<tr>
								<td colspan="3">Test Report No :
								</td>
								<td colspan="3">
									<span t-field="o.eln_id"/>
								</td>
								<td colspan="3">Report Issue Date :
								</td>
								<td colspan="3">
									<span t-field="o.srf_date"/>
								</td>
								<td colspan="3">ULR NO :
								</td>
								<td colspan="3">
									<span t-field="o.sample_id.ulr_no"/>
								</td>
							</tr>
						</table>
							
					</div>
							
				</div>
				
			</t>
		</t>
	</template>
</odoo>