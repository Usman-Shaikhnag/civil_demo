<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="lerm_datasheet_master_form" model="ir.ui.view">
        <field name="name">lerm.datasheet.master.form</field>
        <field name="model">lerm.datasheet.master</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                    <field name="datasheet">
                        <tree editable="bottom">
                            <field name="parameter"/>
                        </tree>
                </field>
                </group>
            </form>
        </field>
        </record>
        
        <record id="lerm_datasheet_master_tree" model="ir.ui.view">
        <field name="name">lerm.datasheet.master.tree</field>
        <field name="model">lerm.datasheet.master</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
        </record>
    </data>
</odoo>